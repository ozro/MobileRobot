robot = NohBot();
plot = DisplacementLinePlotter();
sTime = tic;
global encoderData;
global encoderTime

prevEnc = encoderData;
time0 = prevEnc(3);
while(toc(sTime) < 5)
    robot.move(0.1, 0.1);
    
    enc = robot.GetEncoders;
    l = enc(1);
    r = enc(2);
    prevL = prevEnc(1);
    prevR = prevEnc(2);
    dsl = l-prevL;
    dsr = r-prevR;
    dt = enc(3) - prevEnc(3);
    
    vl = dsl/dt;
    vr = dsr/dt;
    prevEnc = enc;
    
    plot.AddToArrays(enc(3)- time0,vl, vr);
    pause(0.2);
end

robot.move(0, 0);